#include<iostream>
#include <iomanip> 
#include <cstring>

using namespace std;
int main(){
    int number;
    cin >> number;
    for (int i = 0; i < number/10000; i++)
    {
        cout << "*" ;
    }
    cout << endl;
    for (int i = 0; i < number/1000%10; i++)
    {
        cout << "*";

    }
    cout << endl;
    for (int i = 0; i < number/100%10; i++)
    {
        cout << "*";
    }
    cout << endl;
    for (int i = 0; i < number/10%10; i++)
    {
        cout << "*";
    }
    cout << endl;
    for (int i = 0; i < number%10; i++)
    {
        cout << "*";
    }
    cout << endl;
    


}
