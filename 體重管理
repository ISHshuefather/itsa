#include <iostream>
using namespace std;
int main()
{
    int n;
    double BMI;
    cin >> n;
    for (int i = 0; i < n; i++)
    {
        cin >> BMI;
        if (BMI<18.5)
        {
            cout << "體重過輕" << endl;
        }
        else if (BMI>=18.5&&BMI<24)
        {
            cout << "正常" << endl;
        }
        else if (BMI>=24&&BMI<28)
        {
            cout << "體重過重" << endl;
        }
        else{
            cout << "肥胖" << endl;


        }
        
        
        
    }
    
}
