#include <iostream>
#include <math.h>
using namespace std;
int main()
{
    int n,a;
    cin >> n;
    for (int i = 0; i < n; i++)
    {   
        cin >> a;
        if (a > 31)
        {
            cout << "Value of more than 31" << endl;
        }
        else
        {
            long long int result=pow(2,a);
            cout << result << endl;
        }
    }
}
